class NewsComponent {
    constructor(_newsService, _functionsService) {
        this._newsService = _newsService;
        this._functionsService = _functionsService;
        //titre de la section
        this.title = 'News';
    }
    ngOnInit() {
        this.refreshNewsList();
    }
    /**
     * Actualise la liste des news valides à afficher
     */
    refreshNewsList() {
        this._newsService.getValidateNews().subscribe(data => {
            this.newsList = data;
        });
        //this.newsList = this._newsService.getValidateNews();
        //console.log(this.newsList);
    }
    /**
     * Affiche la date au format JJ mois AAAA
     * @param date date à convertir
     */
    dateToDisplay(date) {
        return this._functionsService.dateToStringToDisplay(date);
    }
    /**
     * Retourne le nombre de news à afficher
     */
    numberOfNews() {
        return this.newsList.length;
    }
}
NewsComponent.ɵfac = function NewsComponent_Factory(t) { return new (t || NewsComponent)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](src_app_shared_services_news_service__WEBPACK_IMPORTED_MODULE_1__["NewsService"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](src_app_shared_services_functions_service__WEBPACK_IMPORTED_MODULE_2__["FunctionsService"])); };
