class NumbersComponent {
    constructor(_apiService) {
        this._apiService = _apiService;
        this.title = 'Les chiffres';
    }
    ngOnInit() {
        this.setUserCount();
        this.setRoomsCount();
        this.setCitysCount();
    }
    /**
     * Initialise le nombre total d'utilisateurs (hors personnel)
     */
    setUserCount() {
        this._apiService.countAllUsers().subscribe((data) => {
            this.numberOfUsers = data;
        });
    }
    /**
     * Initialise le nombre total de salles
     */
    setRoomsCount() {
        this._apiService.countAllRooms().subscribe((data) => {
            this.numberOfRooms = data;
        });
    }
    /**
     * Initialise le nombre total de villes
     */
    setCitysCount() {
        this._apiService.countAllCitys().subscribe((data) => {
            this.numberOfCitys = data;
        });
    }
}
