class PhotosEditDisplayComponent {
    constructor(dialogRef, data, _displayParametersService, _functionsService) {
        this.dialogRef = dialogRef;
        this.data = data;
        this._displayParametersService = _displayParametersService;
        this._functionsService = _functionsService;
        this.dialogTitle = "Modifier l'affichage des photos";
        this.btn_cancel_content = "Annuler";
        this.btn_valid_content = "Valider les modifications";
        this.input_numberToDisplay_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](4, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].max(20));
        this.input_numberToDisplay_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](data.nbToDisplay, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].max(20));
    }
    ngOnInit() {
    }
    /**
     * Click sur le bouton 'valider'
     */
    btn_valid_click() {
        let newNbToDisplay = this.input_numberToDisplay_value.value;
        if (newNbToDisplay != null) {
            if (newNbToDisplay <= 20) {
                this.savePicturesEditChanges();
            }
            else {
                this._functionsService.openErrorSnackBar("Maximum 20 photos");
            }
        }
        else {
            this._functionsService.openErrorSnackBar("Vous n'avez rien saisi");
        }
    }
    /**
     * Sauvegarde des modifications du paramètre d'affichage
     */
    savePicturesEditChanges() {
        this._displayParametersService.setDisplayParameterByName(this.data.DP_id, 'pictures', 'random;' + this.input_numberToDisplay_value.value);
        this.dialogRef.close();
    }
}
