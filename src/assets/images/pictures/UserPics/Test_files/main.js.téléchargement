class ReviewsStarsComponent {
    constructor() {
        this.fullStarsToDisplay = [];
        this.emptyStarsToDisplay = [];
    }
    ngOnInit() {
        //Création de tableaux pour afficher le bon nombre d'étoiles vides et pleines dans la vue
