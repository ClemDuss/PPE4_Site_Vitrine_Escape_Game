class ReviewsService {
    constructor(_displayParametersService, _apiService) {
        //this.refreshDisplayReviewsParameter();
        this._displayParametersService = _displayParametersService;
        this._apiService = _apiService;
        this._allReviews = _data_reviews_data__WEBPACK_IMPORTED_MODULE_1__["ReviewsData"];
    }
    /**
     * Actualise les paramètres d'affichage d'après la BDD
     */
    refreshDisplayReviewsParameter() {
        this._displayParametersService.getDisplayParameterByName('reviews').subscribe((someDP) => {
            this._displayReviewsParameter = someDP.parameter;
        });
        this._nbOfReviewsToDisplay = parseInt(this._displayReviewsParameter.split(';')[1]);
    }
    /**
     * Retourne la note moyenne des utilisateurs
     */
    getRateAverage() {
        let subjectAverage = new rxjs__WEBPACK_IMPORTED_MODULE_2__["Subject"]();
        this._apiService.getGlobalRateAverage().subscribe((average) => {
            subjectAverage.next(average);
        });
        return subjectAverage.asObservable();
    }
    /**
     * Retourne tous les avis selon le paramètre d'affichage
     */
    getAllReviews() {
        let allReviewsFromApi = new rxjs__WEBPACK_IMPORTED_MODULE_2__["Subject"]();
        this._displayParametersService.getAllDisplayParameters().subscribe((allDP) => {
            let reviewsParameter = new _models_display_parameters__WEBPACK_IMPORTED_MODULE_3__["DisplayParameters"]('reviews', 'random;4');
            allDP.forEach((someDP) => {
                if (someDP.getDisplayName() == 'reviews') {
                    reviewsParameter = someDP;
                }
            });
            let parameter = reviewsParameter.getParameter().split(';')[0];
            //Affichage selon le paramètre enregistré
            switch (parameter) {
                case 'random':
                    this._apiService.getAllReviews().subscribe((allReviews) => {
                        allReviewsFromApi.next([]);
                        let newAllReviews = [];
                        let nbOfReviews = parseInt(reviewsParameter.getParameter().split(';')[1]);
                        let maxRate = 5;
                        let minRate = 0;
                        let continueToLoop = true;
                        let loopCount = 0;
                        while (continueToLoop) {
                            let randomSelectedReview = allReviews[Math.floor(Math.random() * allReviews.length)];
                            if (newAllReviews.length - 1 != allReviews.length && newAllReviews.length < nbOfReviews && loopCount <= 100) {
                                if (!(newAllReviews.includes(randomSelectedReview))) {
                                    if (minRate <= randomSelectedReview.getRate() && randomSelectedReview.getRate() <= maxRate) {
                                        newAllReviews.push(randomSelectedReview);
                                    }
                                }
                            }
                            else {
                                continueToLoop = false;
                            }
                            loopCount++;
                        }
                        allReviewsFromApi.next(newAllReviews);
                    });
                    break;
                case 'byRating':
                    this._apiService.getAllReviews().subscribe((allReviews) => {
                        allReviewsFromApi.next([]);
                        let newAllReviews = [];
                        let nbOfReviews = parseInt(reviewsParameter.getParameter().split(';')[1]);
                        let maxRate = parseInt(reviewsParameter.getParameter().split(';')[2].split(',')[1]);
                        let minRate = parseInt(reviewsParameter.getParameter().split(';')[2].split(',')[0]);
                        let continueToLoop = true;
                        let loopCount = 0;
                        while (continueToLoop) {
                            let randomSelectedReview = allReviews[Math.floor(Math.random() * allReviews.length)];
                            if (newAllReviews.length - 1 != allReviews.length && newAllReviews.length < nbOfReviews && loopCount <= 100) {
                                if (!(newAllReviews.includes(randomSelectedReview))) {
                                    if (minRate <= randomSelectedReview.getRate() && randomSelectedReview.getRate() <= maxRate) {
                                        newAllReviews.push(randomSelectedReview);
                                    }
                                }
                            }
                            else {
                                continueToLoop = false;
                            }
                            loopCount++;
                        }
                        allReviewsFromApi.next(newAllReviews);
                    });
                    break;
            }
        });
        return allReviewsFromApi.asObservable();
    }
    // GET
    /**
     * Retourne le nombre d'avis à afficher
     */
    getNbOfReviewsToDisplay() {
        this.refreshDisplayReviewsParameter();
        return this._nbOfReviewsToDisplay;
    }
    /**
     * Retourne le paramètre d'affichage
     */
    getSelectedParameter() {
        this.refreshDisplayReviewsParameter();
        return this._displayReviewsParameter;
    }
}
