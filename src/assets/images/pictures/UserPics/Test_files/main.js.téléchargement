class NewsAddComponent {
    constructor(dialogRef, data, _snackBar, _newsService, _functionsService) {
        this.dialogRef = dialogRef;
        this.data = data;
        this._snackBar = _snackBar;
        this._newsService = _newsService;
        this._functionsService = _functionsService;
        //Valeurs par défaut pour l'affichage
        this.dialogTitle = 'Ajouter une nouvelle news';
        this.btn_validate_content = 'Valider';
        this.btn_cancel_content = 'Annuler';
        this.input_title_content = 'Titre';
        this.input_title_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]();
        this.datepicker_start_content = 'Date de début';
        this.datepicker_start_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]();
        this.datepicker_end_content = 'Date de fin';
        this.datepicker_end_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]();
        this.input_description_content = 'Description';
        this.input_description_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"]();
        this.input_description_placeholder = 'Entrez la description de la news';
        this._theNews = new src_app_shared_models_news__WEBPACK_IMPORTED_MODULE_2__["News"]();
        //selon le type d'action, on modifie le titre et le contenu bouttons
        switch (data.type) {
            case 'add':
                this.btn_validate_content = "Valider l'ajout";
                this.dialogTitle = "Ajouter une nouvelle news";
                break;
            case 'edit':
                this.btn_validate_content = "Valider les modifications";
                this.dialogTitle = "Modifier une news";
                this._theNews = data.news;
                this._newsEditId = this._theNews.getId();
                this.input_title_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](this._theNews.getTitle());
                this.datepicker_start_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](this._theNews.getStartDate());
                this.datepicker_end_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](this._theNews.getEndDate());
                this.input_description_value = new _angular_forms__WEBPACK_IMPORTED_MODULE_3__["FormControl"](this._theNews.getDescription());
                break;
        }
    }
    ngOnInit() {
    }
    /**
     * Click sur le bouton de validation du formulaire
     */
    btn_validate_click() {
        if (this.input_title_value.value != null && this.datepicker_end_value.value != null && this.input_description_value.value != null) {
            let theNews;
            theNews = new src_app_shared_models_news__WEBPACK_IMPORTED_MODULE_2__["News"]();
            theNews.setTitle(this.input_title_value.value);
            theNews.setDescription(this.input_description_value.value);
            switch (this.data.type) {
                case 'add':
                    if (this.datepicker_start_value.value != null) {
                        theNews.setStartDate(this.datepicker_start_value.value.toDate());
                    }
                    theNews.setEndDate(this.datepicker_end_value.value.toDate());
                    this._newsService.addNews(theNews);
                    this._functionsService.openSuccessSnackBar("News ajoutée !");
                    break;
                case 'edit':
                    theNews.setId(this._newsEditId);
                    if (this.datepicker_start_value.value != null) {
                        theNews.setStartDate(this.datepicker_start_value.value);
                    }
                    else {
                        theNews.setStartDate(null);
                    }
                    theNews.setEndDate(this.datepicker_end_value.value);
                    this._newsService.editNews(theNews);
                    this._functionsService.openSuccessSnackBar("News modifiée !");
                    break;
            }
            this.dialogRef.close();
        }
        else {
            this._functionsService.openErrorSnackBar("Les champs 'Titre', 'Date de fin' et 'Description' doivent être renseignés !");
        }
    }
    /**
     * Click sur le boutton 'Annuler' du formulaire
     */
    btn_cancel_click() {
        this.dialogRef.close();
    }
}
