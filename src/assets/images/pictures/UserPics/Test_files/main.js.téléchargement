class PicturesComponent {
    constructor(_mediasPubliesService) {
        this._mediasPubliesService = _mediasPubliesService;
        //titre de la section
        this.title = 'Photos';
        //chemin d'accès à l'emplacement de stockage des photos utilisateur
        this.picsFolder = './../../../../assets/images/pictures/UserPics/';
    }
    ngOnInit() {
        this.refreshAllPictures();
    }
    /**
     * Acluatise la liste des photos à afficher
     */
    refreshAllPictures() {
        this._mediasPubliesService.getAllMedias().subscribe((allMedias) => {
            this.allPictures = allMedias;
        });
    }
}
