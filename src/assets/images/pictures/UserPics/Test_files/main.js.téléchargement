class UserReviewsComponent {
    constructor(_reviewsService) {
        this._reviewsService = _reviewsService;
        this.title = 'Avis';
        this.pictoPath = '../../../../assets/images/picto/Stars/';
        this.emptyStar = this.pictoPath + 'star_empty_white.svg';
        this.fullStar = this.pictoPath + 'star_full_white.svg';
        this.middleStar = this.pictoPath + 'star_middle_white.svg';
    }
    ngOnInit() {
        this.refreshAllReviews();
        this.refreshAverage();
    }
    /**
     * Actualise la moyenne de notation
     */
    refreshAverage() {
        this._reviewsService.getRateAverage().subscribe((data) => {
            this.rateAverage = data;
        });
    }
    /**
     * Actualise la liste des avis à afficher
     */
    refreshAllReviews() {
        this._reviewsService.getAllReviews().subscribe((allReviews) => {
            this.allReviews = allReviews;
        });
    }
    /**
     * Retourne le nombre d'avis à afficher
     */
    getNumberOfReviews() {
        return this.userReviewsList.length;
    }
}
