class RoomsComponent {
    constructor(dialog, _roomsService, _citysService) {
        this.dialog = dialog;
        this._roomsService = _roomsService;
        this._citysService = _citysService;
        this.title = 'Salles';
        this.btn_moreInformations_content = "Plus d'infos";
    }
    ngOnInit() {
        this.refreshRoomsList();
    }
    /**
     * Actualise la liste des Salles
     */
    refreshRoomsList() {
        this._citysService.getAllCitys().subscribe((allCitys) => {
            let newRoomsList = [];
            this._allCitys = allCitys;
            allCitys.forEach((someCity) => {
                newRoomsList.push({ idVille: someCity.id, nomVille: someCity.nomVille, nbSalles: 0 });
            });
            this.roomsList = newRoomsList;
            this._roomsService.getAllRooms().subscribe((allRooms) => {
                this.roomsList.forEach(element => {
                    element.nbSalles = 0;
                });
                allRooms.forEach(someRoom => {
                    this.roomsList.forEach(element => {
                        if (element.idVille == someRoom.idVille) {
                            element.nbSalles++;
                        }
                    });
                });
                //portion de code pour ne pas afficher à l'écran les villes qui n'ont pas de salles
                let index = 0;
                let indexToSlice = [];
                this.roomsList.forEach((element) => {
                    if (element.nbSalles == 0) {
                        indexToSlice.push(index);
                    }
                    index++;
                });
                this.roomsList.reverse();
                indexToSlice.reverse();
                indexToSlice.forEach((i) => {
                    this.roomsList.slice(i);
                });
                this.roomsList.reverse();
            });
        });
    }
    openDialog(idVille, nomVille) {
        const dialogRef = this.dialog.open(_room_details_room_details_component__WEBPACK_IMPORTED_MODULE_1__["RoomDetailsComponent"], {
            width: '500px',
            data: { idVille: idVille, nomVille: nomVille }
        });
    }
    btn_moreInformations_click(idVille, nomVille) {
        this.openDialog(idVille, nomVille);
    }
}
RoomsComponent.ɵfac = function RoomsComponent_Factory(t) { return new (t || RoomsComponent)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_material_dialog__WEBPACK_IMPORTED_MODULE_2__["MatDialog"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](src_app_shared_services_rooms_service__WEBPACK_IMPORTED_MODULE_3__["RoomsService"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](src_app_shared_services_citys_service__WEBPACK_IMPORTED_MODULE_4__["CitysService"])); };
