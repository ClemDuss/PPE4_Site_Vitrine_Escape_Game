class NewsEditComponent {
    constructor(dialog, _newsService, _loginService) {
        this.dialog = dialog;
        this._newsService = _newsService;
        this._loginService = _loginService;
        this.welcomeText = 'Gestion des news';
        this.btn_addNews_content = 'Ajouter une news';
        this.btn_addNews_icon = 'add';
    }
    ngOnInit() {
        //On vérifie si l'utilisateur est connecté
        this._loginService.isUserConnected();
        this.setNoteBodyRightHeight();
    }
    /**
     * Click sur le bouton d'ajout de News
     */
    btn_addNews_click() {
        this.openDialog();
    }
    /**
     * Ouvre le modal pour ajouter une News
     */
    openDialog() {
        this._newsService.setSelectedId(null);
        const dialogRef = this.dialog.open(_news_add_news_add_component__WEBPACK_IMPORTED_MODULE_1__["NewsAddComponent"], {
            width: '500px',
            data: { type: 'add', news: new src_app_shared_models_news__WEBPACK_IMPORTED_MODULE_2__["News"]() }
        });
    }
    /**
     * Définition d'une portion de l'affichage du 'Post-it'
     */
    setNoteBodyRightHeight() {
        document.getElementById('bodyRight').style.height = document.getElementById('content').style.height;
    }
}
